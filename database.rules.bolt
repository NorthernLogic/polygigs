
path / {
  read() { false }
  write() { false }
}

path /gigs {
  index() {[
    "isActive",
    "dateNotificationSent"
  ]}
  read() { true }
  write() { false }
}

path /notificationRegistrations {
  index() {[
    "registered"
  ]}
}

path /notificationRegistrations/{id} is NotificationRegistration {
  read() { false }
  write() { prior(this) == null }
}

type NotificationRegistration {
  registered: Boolean,
  dateRegistered: String,
}